<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<StackPane prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" stylesheets="@ProgramExecution.css">
   <HBox prefHeight="600.0" prefWidth="778.0">
      <VBox prefHeight="200.0" prefWidth="100.0">
         <HBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="46.0" prefWidth="247.0">
            <Label prefHeight="61.0" prefWidth="166.0" text="Number of program states">
               <font>
                  <Font name="Calisto MT Bold" size="14.0" />
               </font>
               <opaqueInsets>
                  <Insets left="5.0" />
               </opaqueInsets>
               <HBox.margin>
                  <Insets left="5.0" top="5.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="programStatesNumberTextField" focusTraversable="false" prefHeight="25.0" prefWidth="50.0">
               <HBox.margin>
                  <Insets left="10.0" top="13.0" />
               </HBox.margin>
               <font>
                  <Font name="Calisto MT" size="14.0" />
               </font>
            </TextField>
         </HBox>
         <VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="218.0" prefWidth="268.0">
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="29.0" prefWidth="74.0" text="Heap Table">
               <font>
                  <Font name="Calisto MT Bold" size="14.0" />
               </font>
               <VBox.margin>
                  <Insets left="5.0" />
               </VBox.margin>
            </Label>
            <TableView fx:id="heapTableView" prefHeight="200.0" prefWidth="200.0">
               <columns>
                  <TableColumn fx:id="heapAddress" prefWidth="133.59992825984955" text="Address" />
                  <TableColumn fx:id="heapValue" prefWidth="132.80004119873047" text="Value" />
               </columns>
            </TableView>
         </VBox>
         <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="34.0" prefWidth="96.0" text="Symbol Table">
            <font>
               <Font name="Calisto MT Bold" size="14.0" />
            </font>
            <VBox.margin>
               <Insets left="5.0" />
            </VBox.margin>
         </Label>
         <TableView fx:id="symbolTableView" prefHeight="200.0" prefWidth="200.0">
            <columns>
               <TableColumn fx:id="symbolTableVariableName" prefWidth="133.6000121831894" text="Name" />
               <TableColumn fx:id="symbolTableVariableValue" prefWidth="133.60000610351562" text="Value" />
            </columns>
            <VBox.margin>
               <Insets bottom="10.0" />
            </VBox.margin>
         </TableView>
         <Button fx:id="runOneStepButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" prefHeight="59.0" prefWidth="109.0" text="Run one step">
            <font>
               <Font name="Calisto MT Bold" size="14.0" />
            </font>
            <VBox.margin>
               <Insets left="75.0" />
            </VBox.margin>
         </Button>
      </VBox>
      <VBox prefHeight="502.0" prefWidth="231.0">
         <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="35.0" prefWidth="54.0" text="Output">
            <font>
               <Font name="Calisto MT Bold" size="14.0" />
            </font>
            <VBox.margin>
               <Insets top="35.0" />
            </VBox.margin>
         </Label>
         <ListView id="outputListView" fx:id="outputListView" prefHeight="194.0" prefWidth="231.0" />
         <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="34.0" prefWidth="86.0" text="File Table">
            <font>
               <Font name="Calisto MT Bold" size="14.0" />
            </font>
         </Label>
         <ListView fx:id="fileTableListView" prefHeight="199.0" prefWidth="231.0" />
      </VBox>
      <VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="502.0" prefWidth="400.0">
         <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="36.0" prefWidth="170.0" text="Program State Identifiers">
            <font>
               <Font name="Calisto MT Bold" size="14.0" />
            </font>
            <VBox.margin>
               <Insets top="35.0" />
            </VBox.margin>
         </Label>
         <ListView fx:id="programStateIdentListView" prefHeight="193.0" prefWidth="218.0" />
         <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="33.0" prefWidth="135.0" text="Execution Stack" textAlignment="CENTER">
            <font>
               <Font name="Calisto MT Bold" size="14.0" />
            </font>
         </Label>
         <ListView fx:id="exeStackListView" prefHeight="198.0" prefWidth="224.0" />
         <HBox.margin>
            <Insets right="5.0" />
         </HBox.margin>
      </VBox>
   </HBox>
</StackPane>
